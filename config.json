{
    "baseURL": "https://wedding.michaelgreenhill.net",
    "languageCode": "en-us",
    "title": "Freya & Michael's Wedding",
    "show_comments": false,
    "disableKinds": [
        "sitemap",
        "RSS"
    ],
    "markup": {
        "defaultMarkdownHandler": "goldmark",
        "goldmark": {
            "extensions": {
                "definitionList": true,
                "footnote": true,
                "linkify": true,
                "linkifyProtocol": "https",
                "strikethrough": true,
                "table": true,
                "taskList": true,
                "typographer": true
            },
            "parser": {
                "attribute": {
                    "block": false,
                    "title": true
                },
                "autoDefinitionTermID": false,
                "autoHeadingID": true,
                "autoIDType": "github",
                "wrapStandAloneImageWithinParagraph": true
            },
            "renderer": {
                "hardWraps": false,
                "unsafe": true,
                "xhtml": false
            }
        }
    },
    "params": {
        "background_image_1": "https://michaelgreenhill-net.s3.ap-southeast-2.amazonaws.com/cdn/wedding/PXL_20240427_035324422.jpg",
        "background_image_2": "https://michaelgreenhill-net.s3.ap-southeast-2.amazonaws.com/cdn/wedding/PXL_20240627_092239359.MP.jpg",
        "background_image_3": "https://michaelgreenhill-net.s3.ap-southeast-2.amazonaws.com/cdn/wedding/FB_IMG_1683706950251.jpg"
    },
    "module": {
        "imports": [{
                "path": "github.com/hugomods/icons/vendors/bootstrap"
            },
            {
                "path": "github.com/hugomods/icons/vendors/font-awesome"
            },
            {
                "path": "github.com/hugomods/icons/vendors/mdi"
            }
        ]
    }
}